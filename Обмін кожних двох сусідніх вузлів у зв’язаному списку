class ListNode:
    def __init__(self, val=0, next=None):
        self.val = val
        self.next = next
def swapPairs(head: ListNode) -> ListNode:
    dummy = ListNode(0)
    dummy.next = head
    prev = dummy

    while prev.next and prev.next.next:
        a = prev.next
        b = a.next

        prev.next, a.next, b.next = b, b.next, a
        prev = a

    return dummy.next 

head = ListNode(1, ListNode(2, ListNode(3, ListNode(4))))

def print_list(node):
    res = []
    while node:
        res.append(node.val)
        node = node.next
    print(res)


